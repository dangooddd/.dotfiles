// Zed settings
{
  "agent": {
    "default_profile": "write",
    "default_model": {
      "provider": "deepseek",
      "model": "deepseek-reasoner"
    }
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "terminal": {
    "line_height": "standard"
  },
  "buffer_font_family": "IosevkaTerm Nerd Font",
  "theme": {
    "mode": "system",
    "light": "Gruvbox Light Soft",
    "dark": "Gruvbox Dark Hard"
  },
  "vim_mode": true,
  "cursor_blink": false,
  "show_edit_predictions": false,
  "languages": {
    "Python": {
      "formatter": [
        {
          "language_server": {
            "name": "ruff"
          }
        }
      ]
    }
  }
}
